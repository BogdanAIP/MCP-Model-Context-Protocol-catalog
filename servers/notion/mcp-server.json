{
  "name": "notion_mcp",
  "version": "1.0.0",
  "base_url": "https://api.notion.com/v1",
  "auth": {
    "type": "bearer",
    "env": "NOTION_TOKEN"
  },
  "headers": {
    "Notion-Version": "2022-06-28"
  },
  "capabilities": [
    {
      "tool": "notion_mcp.create_page",
      "method": "POST",
      "path": "/pages",
      "inputs": {
        "body": "object"
      }
    }
  ],
  "meta": {
    "title": "Notion",
    "description": "Создание страниц и базы данных",
    "category": "productivity",
    "homepage": "https://developers.notion.com/",
    "repo": "https://github.com",
    "license": "MIT",
    "badges": [
      "community"
    ]
  }
}